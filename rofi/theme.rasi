* {
  spacing: 0;
  width: 17%;
  background-color: #181818;
  text-color: #b8b8b8;
}

window {
  border: 3px;
  border-color: #585858;
}

prompt {
  padding: 16px;
}

entry {
  padding: 16px 0px;
}

element {
  padding: 8px;
}

element-icon {
  size: 2ch;
}

element selected, element-text selected, element-icon selected {
  background-color: #383838;
}

listview {
  lines: 7;
  columns: 1;
}
